{
  "expo": {
    "name": "RecruitRadar",
    "slug": "recruitradar",
    "version": "1.0.0",
    "scheme": "recruitradar",
    "orientation": "portrait",
    "icon": "./src/assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./src/assets/splash.png",
      "package": "com.anonymous.recruitradar"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "794d8da1-a345-42fb-81e4-1f499fec4d8b"
      }
    }
  }
}
